function getMilk(money, priceBottle){
    
    console.log('Buy ' + calcBottles(money, priceBottle) + ' bottles of milk.');
//     var change = money % 1.5;
    

    return calcChange(money, priceBottle)
}



function calcBottles(startMoney, priceBottle){
    var noBottles = Math.floor(startMoney / priceBottle);
    return noBottles;
}

function calcChange(startMoney, priceBottle){
    var change = startMoney % priceBottle;
    return change;
}

// var change = getMilk(20);
console.log('Your change is $' + getMilk(20, 1.5) + '.');


function getBMI(weight, height){
    var bmi = weight / (height ** 2);
    return bmi;

}

var BMI = getBMI(150,1.778);
console.log(Math.round(BMI));
